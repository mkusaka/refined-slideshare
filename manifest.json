{
  "manifest_version": 3,
  "name": "refined slideshare",
  "version": "0.14",
  "description": "make slideshare's user experience better",
  "permissions": [
    "activeTab",
    "cookies"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.slideshare.net/*"
      ],
      "js": ["content.js"]
    }
  ],
  "host_permissions": [
    "https://www.slideshare.net/*"
  ]
}
